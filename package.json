{
  "name": "ember-exif",
  "version": "0.1.5",
  "description": "Parse exif data from images and add it to the application as a importable module.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js config lib tests",
    "lint:js:fix": "eslint ./*.js config tests --fix",
    "start": "ember serve",
    "test": "ember try:each"
  },
  "engines": {
    "node": ">= 6.9",
    "yarn": "YARN NO LONGER USED - use npm instead."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffjewiss/ember-exif"
  },
  "bugs": {
    "url": "https://github.com/jeffjewiss/ember-exif/issues"
  },
  "homepage": "https://github.com/jeffjewiss/ember-exif",
  "author": "Jeff Jewiss <jeffjewiss@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "broccoli-funnel": "2.0.1",
    "broccoli-merge-trees": "3.0.0",
    "broccoli-plugin": "1.3.0",
    "dist-exiftool": "10.53.0",
    "ember-cli-babel": "6.14.1",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "merge": "1.2.0",
    "node-exiftool": "2.3.0",
    "prettyjson": "1.2.1"
  },
  "keywords": [
    "ember-addon",
    "exif",
    "image",
    "photo"
  ],
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "~3.3.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.3.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "loader.js": "^4.2.3"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-sri",
      "broccoli-asset-rev"
    ]
  }
}
